<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_background"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">



    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/manage_layout_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/manage_padding_left"
        android:orientation="horizontal"
        android:id="@+id/pwd_reset_layout">

        <ImageView
            android:id="@+id/owner_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/pwd_reset">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_toRightOf="@+id/owner_img"
            android:layout_marginLeft="@dimen/manage_margin_left"
            android:text="@string/psw_reset"/>

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/manage_margin_right"
            android:src="@mipmap/right">

        </ImageView>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/manage_layout_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/manage_padding_left"
        android:layout_marginTop="@dimen/manage_margin_top"
        android:orientation="horizontal"
        android:id="@+id/user_name_reset_layout">

        <ImageView
            android:id="@+id/tenement_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/username_reset">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_toRightOf="@+id/tenement_img"
            android:layout_marginLeft="@dimen/manage_margin_left"
            android:text="@string/user_name_reset"/>

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/manage_margin_right"
            android:src="@mipmap/right">

        </ImageView>

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/manage_layout_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/manage_padding_left"
        android:layout_marginTop="@dimen/manage_margin_top"
        android:orientation="horizontal"
        android:id="@+id/feed_back_layout">

        <ImageView
            android:id="@+id/car_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/feedback">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_toRightOf="@+id/car_img"
            android:layout_marginLeft="@dimen/manage_margin_left"
            android:text="@string/feedback"/>

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/manage_margin_right"
            android:src="@mipmap/right">

        </ImageView>

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/manage_layout_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/manage_padding_left"
        android:layout_marginTop="@dimen/manage_margin_top"
        android:orientation="horizontal"
        android:id="@+id/link_people_layout">

        <ImageView
            android:id="@+id/owner_apply_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/link_pepple">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_toRightOf="@+id/owner_apply_img"
            android:layout_marginLeft="@dimen/manage_margin_left"
            android:text="@string/link_people"/>

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/manage_margin_right"
            android:src="@mipmap/right">

        </ImageView>

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/manage_layout_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/manage_padding_left"
        android:layout_marginTop="@dimen/manage_margin_top"
        android:orientation="horizontal"
        android:id="@+id/about_layout">

        <ImageView
            android:id="@+id/house_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/about">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_toRightOf="@+id/house_img"
            android:layout_marginLeft="@dimen/manage_margin_left"
            android:text="@string/about"/>

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/manage_margin_right"
            android:src="@mipmap/right">

        </ImageView>

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/manage_layout_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/manage_padding_left"
        android:layout_marginTop="@dimen/manage_margin_top"
        android:orientation="horizontal"
        android:id="@+id/login_out_layout">

        <ImageView
            android:id="@+id/login_out_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/about">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_toRightOf="@+id/login_out_img"
            android:layout_marginLeft="@dimen/manage_margin_left"
            android:text="@string/loginout"/>

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/manage_margin_right"
            android:src="@mipmap/right">

        </ImageView>

    </RelativeLayout>







</LinearLayout>