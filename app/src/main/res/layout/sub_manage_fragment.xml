<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_background"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">



    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/manage_layout_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/manage_padding_left"
        android:orientation="horizontal"
        android:id="@+id/owner_manage_layout">

        <ImageView
            android:id="@+id/owner_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/owner_manage">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_toRightOf="@+id/owner_img"
            android:layout_marginLeft="@dimen/manage_margin_left"
            android:text="@string/sub_tenement_manage_str"/>

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/manage_margin_right"
            android:src="@mipmap/right">

        </ImageView>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/manage_layout_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/manage_padding_left"
        android:layout_marginTop="@dimen/manage_margin_top"
        android:orientation="horizontal"
        android:id="@+id/tenement_manage_layout">

        <ImageView
            android:id="@+id/tenement_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/tenement_manage">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_toRightOf="@+id/tenement_img"
            android:layout_marginLeft="@dimen/manage_margin_left"
            android:text="@string/sub_community_manage_str"/>

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/manage_margin_right"
            android:src="@mipmap/right">

        </ImageView>

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/manage_layout_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/manage_padding_left"
        android:layout_marginTop="@dimen/manage_margin_top"
        android:orientation="horizontal"
        android:id="@+id/owner_apply_manage_layout">

        <ImageView
            android:id="@+id/owner_apply_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/owner_apply_manage">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_toRightOf="@+id/owner_apply_img"
            android:layout_marginLeft="@dimen/manage_margin_left"
            android:text="@string/sub_certificate_manage_str"/>

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/manage_margin_right"
            android:src="@mipmap/right">

        </ImageView>

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/manage_layout_height"
        android:background="@color/white"
        android:paddingLeft="@dimen/manage_padding_left"
        android:layout_marginTop="@dimen/manage_margin_top"
        android:orientation="horizontal"
        android:id="@+id/house_manage_layout">

        <ImageView
            android:id="@+id/house_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/house_manage">

        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_toRightOf="@+id/house_img"
            android:layout_marginLeft="@dimen/manage_margin_left"
            android:text="@string/sub_data_manage_str"/>

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/manage_margin_right"
            android:src="@mipmap/right">

        </ImageView>

    </RelativeLayout>



